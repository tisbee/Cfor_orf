			self.output_2(p)
		elif(p == self.typeConversion("Disgust")):
			self.output_3(p)
		elif(p == self.typeConversion("Sadness")):
			self.output_4(p)
		elif(p == self.typeConversion("Surprise")):
			self.output_5(p)
		elif(p == self.typeConversion("Excellent")):
			self.output_6(p)
		elif(p == self.typeConversion("GutsPose")):
			self.output_7(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
